[
    {
        "portMappings": [
            {
                "hostPort": 80,
                "protocol": "tcp",
                "containerPort": 80
            }
        ],
        "environment": [
            {
              "name": "RDS_ENDPOINT",
              "value": "RDS_ENDPOINT_VALUE"
            }
        ],
        "linuxParameters": {
            "capabilities": {
                "add": [
                    "SYS_PTRACE"
                ]
            },
            "initProcessEnabled": true
        },
        "cpu": 0,
        "image": "public.ecr.aws/m5k9u8m8/aws-goat-m2:latest",
        "name": "aws-goat-m2",
        "privileged": true,
        "mountPoints": [
            {
                "sourceVolume": "modules",
                "containerPath": "/lib/modules"
            },
            {
                "sourceVolume": "kernels",
                "containerPath": "/usr/src/kernels"
            }
        ]
    }
]
